{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "ignorePatterns": ["dist/", "node_modules/"],
    "rules": {
        "prettier/prettier": 0,
        "camelcase": ["error"],
        "eqeqeq": ["error", "always"]
    }
}